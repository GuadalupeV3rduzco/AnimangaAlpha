{
  "expo": {
    "name": "Animanga Plus",
    "slug": "AnimangaApp",
    "version": "0.6.3",
    "orientation": "portrait",
    "icon": "./assets/images/animangalogo.png",
    "scheme": "animangaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.animangaapp.plus",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/animangalogo2.png",
        "backgroundImage": "./assets/images/fondo.png",
        "monochromeImage": "./assets/images/animangamonochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/animangalogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/animangaplus.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "446da723-b511-4bde-b60a-92e96deb158b"
      }
    }
  }
}
